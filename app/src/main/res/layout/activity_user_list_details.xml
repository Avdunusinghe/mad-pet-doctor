<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserListDetailsActivity">

    <TextView
        android:id="@+id/user_name_list_textview"
        android:layout_width="255dp"
        android:layout_height="28dp"
        android:text="@string/user_name_list_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/user_email_list_textview"
        android:layout_width="256dp"
        android:layout_height="27dp"
        android:layout_marginTop="8dp"
        android:text="@string/user_email_list_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_name_list_textview" />

    <TextView
        android:id="@+id/user_phone_list_textview"
        android:layout_width="252dp"
        android:layout_height="26dp"
        android:layout_marginTop="8dp"
        android:text="@string/user_phone_list_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_email_list_textview" />

</androidx.constraintlayout.widget.ConstraintLayout>