<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserListDetailsActivity">

    <TextView
        android:id="@+id/user_name_list_textview"
        android:layout_width="351dp"
        android:layout_height="26dp"
        android:layout_marginTop="248dp"
        android:text="@string/user_name_list_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/user_email_list_textview"
        android:layout_width="346dp"
        android:layout_height="26dp"
        android:layout_marginTop="12dp"
        android:text="@string/user_email_list_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_name_list_textview" />

    <TextView
        android:id="@+id/user_phone_list_textview"
        android:layout_width="352dp"
        android:layout_height="29dp"
        android:layout_marginTop="16dp"
        android:text="@string/user_phone_list_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_email_list_textview" />
</androidx.constraintlayout.widget.ConstraintLayout>