<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MedicalCenterListDetailActivity">

    <TextView
        android:id="@+id/medicalcenter_list_name_textview"
        android:layout_width="323dp"
        android:layout_height="42dp"
        android:layout_marginTop="16dp"
        android:text="@string/medicalcenter_list_name_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.34"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/medicalcenter_list_address_textview"
        android:layout_width="315dp"
        android:layout_height="42dp"
        android:layout_marginTop="12dp"
        android:text="@string/medicalcenter_list_address_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.309"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalcenter_list_name_textview" />

    <TextView
        android:id="@+id/medicalcenter_list_mobilenumber_textview"
        android:layout_width="315dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:text="@string/medicalcenter_list_mobilenumber_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.302"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalcenter_list_address_textview" />

    <TextView
        android:id="@+id/medicalcenter_list_email_textview"
        android:layout_width="306dp"
        android:layout_height="38dp"
        android:layout_marginTop="12dp"
        android:text="@string/medicalcenter_list_email_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.276"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalcenter_list_mobilenumber_textview" />
</androidx.constraintlayout.widget.ConstraintLayout>