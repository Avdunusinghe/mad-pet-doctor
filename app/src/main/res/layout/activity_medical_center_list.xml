                android:layout_weight="1"
                android:text=""
                android:textColor="@color/black" />

            <ImageView
                android:id="@+id/deletebtn2"
                android:layout_width="39dp"
                android:layout_height="26dp"
                app:srcCompat="@drawable/delete_btn" />
        </TableRow>

        <TableRow

            android:padding="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="3"
                android:textColor="@color/black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="center3"
                android:textColor="@color/black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="center3@gmail.com"
                android:textColor="@color/black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text=""
                android:textColor="@color/black" />

            <ImageView
                android:id="@+id/deletebtn3"
                android:layout_width="39dp"
                android:layout_height="26dp"
                app:srcCompat="@drawable/delete_btn" />
        </TableRow>

        <TableRow

            android:padding="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="4"
                android:textColor="@color/black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="center4"
                android:textColor="@color/black" />

