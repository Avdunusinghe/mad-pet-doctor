        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cont_phone"
        android:layout_width="138dp"
        android:layout_height="76dp"
        android:layout_marginTop="275dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:contentDescription="@string/cont_phone"
        android:text="         Contact No                        071-1122321 "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cont_label1"
        android:layout_width="78dp"
        android:layout_height="49dp"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="40dp"
        android:contentDescription="@string/cont_label1"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cont_email" />

    <TextView
        android:id="@+id/cont_label2"
        android:layout_width="74dp"
        android:layout_height="42dp"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="36dp"
        android:contentDescription="@string/cont_label2"
        android:text="Email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cont_label1" />

    <TextView
        android:id="@+id/cont_label3"
        android:layout_width="78dp"
        android:layout_height="41dp"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="56dp"
        android:contentDescription="@string/cont_label3"
        android:text="Message"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cont_label2" />

    <Button
        android:id="@+id/bton3"
        android:layout_width="140dp"
        android:layout_height="43dp"
        android:layout_marginBottom="24dp"
        android:contentDescription="@string/bton3"
        android:text="SUBMIT"
        app:backgroundTint="@android:color/holo_green_dark"
        app:iconTint="#208A24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TableRow
        android:id="@+id/tableRow"
        android:layout_width="180dp"
        android:layout_height="47dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="24dp"
        android:background="#E8DFDF"
        app:layout_constraintStart_toEndOf="@+id/cont_label1"
        app:layout_constraintTop_toTopOf="@+id/profbar3" />

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="182dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="36dp"
        android:background="#E8DEDE"
        app:layout_constraintStart_toEndOf="@+id/cont_label2"
        app:layout_constraintTop_toBottomOf="@+id/tableRow" />

    <TableRow
        android:layout_width="192dp"
        android:layout_height="84dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="32dp"
        android:background="#E6DBDB"
        app:layout_constraintStart_toEndOf="@+id/cont_label3"
        app:layout_constraintTop_toBottomOf="@+id/tableRow2" />

</androidx.constraintlayout.widget.ConstraintLayout>